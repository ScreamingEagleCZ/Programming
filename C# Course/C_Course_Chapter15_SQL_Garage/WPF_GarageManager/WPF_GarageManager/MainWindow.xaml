<Window x:Class="WPF_GarageManager.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_GarageManager"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="725">
    <Grid>
        <Label Content="Garage List" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <ListBox Name="listGarages" HorizontalAlignment="Left" Margin="10,41,0,268" Height="175"  Width="158" SelectionChanged="listGarages_SelectionChanged"/>
        <Label Content="Associated Cars List" HorizontalAlignment="Left" Margin="204,10,0,0" VerticalAlignment="Top"/>
        <ListBox x:Name="listAssociatedCars" HorizontalAlignment="Left" Margin="204,41,0,268" Height="175"  Width="158"/>

    </Grid>
</Window>
